	<!-- Reglas de Navegacion -->
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>entrar</from-outcome>
			<to-view-id>/pages/menu.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>menu.xhtml</display-name>
		<from-view-id>/pages/menu.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>salir</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
